{% extends "base.djhtml" %}
{% block scripts %}
{{ block.super }}
{% endblock %}
{% block workspace %}
<h3> Статьи </h3>
<br>
{% for article in articles %}
<div>
	Автор: {{ article.author }}<br>
	Название: {{ article.title }}<br>
	<p class='simple'>
		{{ article.body|safe }}
	</p>
</div>
{% endfor %}
{% endblock %}
